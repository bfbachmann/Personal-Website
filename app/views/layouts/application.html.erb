<!DOCTYPE html>
<html>
<head>
	<title>Blog</title>
	<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
	<%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
	<%= csrf_meta_tags %>

	<link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" rel="stylesheet">
	<link rel="stylesheet" type="text/css" href="//fonts.googleapis.com/css?family=Open+Sans">
	<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

</head>

<body>

<nav class="navbar navbar-custom">
	<div class="container-fluid">
		<div class="navbar-header">
		  <a class="navbar-brand" href="/welcome/index"> Home </a>
		</div>
		<ul class="nav navbar-nav">
			<li> <%= link_to 'Articles', articles_path %> </li> 
			<li> <%= link_to 'About', static_pages_about_path %> </li> 
			<li> <%= link_to 'Projects', static_pages_projects_path %> </li> 
		</ul>
		<ul class="nav navbar-nav navbar-right">
			<li> <%= link_to '', static_pages_contact_path, :class => "glyphicon glyphicon-envelope" %> </li> 
		</ul>
	</div>
</nav>

<div class="center-column container-fluid">
	<%= yield %>
</div>

</body>
</html>
